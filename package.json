    "@types/react": "18",
    "@types/react-dom": "18",
    "babel-jest": "^29.7.0",
    "babel-preset-react-app": "^10.0.1",
    "eslint": "^8.56.0",
    "eslint-config-react-app": "^7.0.1",
    "jest": "^29.7.0",
