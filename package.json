{
  "name": "musae",
  "version": "1.0.14-beta.4",
  "description": "musae-ui",
  "author": "tutu@fantufantu.com",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "prepublishOnly": "z rm dist && pnpm run build",
    "preinstall": "npx @aiszlab/watchdog preinstall",
    "authors": "zx scripts/contributors.mjs",
    "changesets": "z cs",
    "version": "z cs version",
    "storybook": "npm run build && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "style": "./dist/styles.css",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "node": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./styles.css": "./dist/styles.css",
    "./locales": {
      "types": "./dist/locale/locales/index.d.ts",
      "require": "./dist/locale/locales/index.cjs",
      "node": "./dist/locale/locales/index.cjs",
      "import": "./dist/locale/locales/index.mjs",
      "default": "./dist/locale/locales/index.mjs"
    },
    "./icons": {
      "types": "./dist/components/icon/icons/index.d.ts",
      "require": "./dist/components/icon/icons/index.cjs",
      "node": "./dist/components/icon/icons/index.cjs",
      "import": "./dist/components/icon/icons/index.mjs",
      "default": "./dist/components/icon/icons/index.mjs"
    },
    "./icons/*": {
      "types": "./dist/components/icon/icons/*/index.d.ts",
      "require": "./dist/components/icon/icons/*/index.cjs",
      "node": "./dist/components/icon/icons/*/index.cjs",
      "import": "./dist/components/icon/icons/*/index.mjs",
      "default": "./dist/components/icon/icons/*/index.mjs"
    },
    "./rich-text-editor": {
      "types": "./dist/components/rich-text-editor/rich-text-editor.d.ts",
      "require": "./dist/components/rich-text-editor/rich-text-editor.cjs",
      "node": "./dist/components/rich-text-editor/rich-text-editor.cjs",
      "import": "./dist/components/rich-text-editor/rich-text-editor.mjs",
      "default": "./dist/components/rich-text-editor/rich-text-editor.mjs"
    },
    "./types/*": "./dist/types/*.d.ts"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10"
  },
  "dependencies": {
    "@aiszlab/fuzzy": "^2.0.6",
    "@aiszlab/relax": "2.0.8-beta.1",
    "@babel/runtime": "^7.28.4",
    "@floating-ui/dom": "^1.7.4",
    "@lexical/code": "^0.35.0",
    "@lexical/headless": "^0.35.0",
    "@lexical/html": "^0.35.0",
    "@lexical/link": "^0.35.0",
    "@lexical/list": "^0.35.0",
    "@lexical/markdown": "^0.35.0",
    "@lexical/react": "^0.35.0",
    "@lexical/rich-text": "^0.35.0",
    "@lexical/selection": "^0.35.0",
    "@lexical/utils": "^0.35.0",
    "@tanstack/react-table": "^8.21.3",
    "dayjs": "^1.11.18",
    "lexical": "^0.35.0",
    "motion": "^12.23.16",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@aiszlab/jarvis": "^1.0.3",
    "@babel/core": "^7.28.4",
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@eslint/js": "^9.36.0",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@storybook/addon-docs": "^9.1.9",
    "@storybook/addon-onboarding": "^9.1.9",
    "@storybook/react-vite": "^9.1.9",
    "@stylexjs/babel-plugin": "^0.15.4",
    "@stylexjs/rollup-plugin": "^0.15.4",
    "@stylexjs/stylex": "^0.15.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.5.2",
    "@types/react": "^19.1.13",
    "@types/react-dom": "^19.1.9",
    "babel-jest": "^30.1.2",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-storybook": "^9.1.9",
    "jest": "^30.1.3",
    "jest-environment-jsdom": "^30.1.2",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "rollup": "^4.52.0",
    "storybook": "^9.1.9",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.0",
    "zx": "^8.8.2"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://aisz.dev/",
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "trailingComma": "all"
  }
}
