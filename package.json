{
  "name": "musae",
  "version": "1.0.25-beta.2",
  "description": "musae-ui",
  "author": "tutu@fantufantu.com",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "rollup -c -w --environment NODE_ENV:development",
    "build": "rollup -c --environment NODE_ENV:production",
    "prepublishOnly": "z rm dist && pnpm run build",
    "preinstall": "npx @aiszlab/watchdog preinstall",
    "authors": "zx scripts/contributors.mjs",
    "changesets": "z cs",
    "version": "z cs version",
    "storybook": "npm run build && storybook dev -p 6006",
    "build-storybook": "storybook build",
    "lint": "eslint ."
  },
  "exports": {
    ".": {
      "style": "./dist/styles.css",
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "node": "./dist/index.cjs",
      "import": "./dist/index.mjs",
      "default": "./dist/index.mjs"
    },
    "./styles.css": "./dist/styles.css",
    "./locales": {
      "types": "./dist/locale/locales/index.d.ts",
      "require": "./dist/locale/locales/index.cjs",
      "node": "./dist/locale/locales/index.cjs",
      "import": "./dist/locale/locales/index.mjs",
      "default": "./dist/locale/locales/index.mjs"
    },
    "./icons": {
      "types": "./dist/components/icon/icons/index.d.ts",
      "require": "./dist/components/icon/icons/index.cjs",
      "node": "./dist/components/icon/icons/index.cjs",
      "import": "./dist/components/icon/icons/index.mjs",
      "default": "./dist/components/icon/icons/index.mjs"
    },
    "./icons/*": {
      "types": "./dist/components/icon/icons/*/index.d.ts",
      "require": "./dist/components/icon/icons/*/index.cjs",
      "node": "./dist/components/icon/icons/*/index.cjs",
      "import": "./dist/components/icon/icons/*/index.mjs",
      "default": "./dist/components/icon/icons/*/index.mjs"
    },
    "./rich-text-editor": {
      "types": "./dist/components/rich-text-editor/rich-text-editor.d.ts",
      "require": "./dist/components/rich-text-editor/rich-text-editor.cjs",
      "node": "./dist/components/rich-text-editor/rich-text-editor.cjs",
      "import": "./dist/components/rich-text-editor/rich-text-editor.mjs",
      "default": "./dist/components/rich-text-editor/rich-text-editor.mjs"
    },
    "./types/*": "./dist/types/*.d.ts"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10"
  },
  "dependencies": {
    "@aiszlab/fuzzy": "^2.1.1",
    "@aiszlab/relax": "^2.1.1",
    "@babel/runtime": "^7.28.6",
    "@floating-ui/dom": "^1.7.5",
    "@lexical/code": "^0.40.0",
    "@lexical/headless": "^0.40.0",
    "@lexical/html": "^0.40.0",
    "@lexical/link": "^0.40.0",
    "@lexical/list": "^0.40.0",
    "@lexical/markdown": "^0.40.0",
    "@lexical/react": "^0.40.0",
    "@lexical/rich-text": "^0.40.0",
    "@lexical/selection": "^0.40.0",
    "@lexical/utils": "^0.40.0",
    "@tanstack/react-table": "^8.21.3",
    "dayjs": "^1.11.19",
    "lexical": "^0.40.0",
    "motion": "^12.34.0",
    "rxjs": "^7.8.2"
  },
  "devDependencies": {
    "@aiszlab/jarvis": "^1.0.3",
    "@babel/core": "^7.29.0",
    "@babel/plugin-transform-runtime": "^7.29.0",
    "@babel/preset-env": "^7.29.0",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@eslint/js": "^10.0.1",
    "@rollup/plugin-babel": "^6.1.0",
    "@rollup/plugin-commonjs": "^29.0.0",
    "@rollup/plugin-node-resolve": "^16.0.3",
    "@rollup/plugin-typescript": "^12.3.0",
    "@storybook/addon-docs": "^10.2.8",
    "@storybook/addon-onboarding": "^10.2.8",
    "@storybook/react-vite": "^10.2.8",
    "@stylexjs/babel-plugin": "^0.17.5",
    "@stylexjs/rollup-plugin": "^0.17.5",
    "@stylexjs/stylex": "^0.17.5",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.2.3",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "babel-jest": "^30.2.0",
    "eslint": "^10.0.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-storybook": "^10.2.8",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "rollup": "^4.57.1",
    "storybook": "^10.2.8",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.55.0",
    "zx": "^8.8.5"
  },
  "peerDependencies": {
    "react": ">=18",
    "react-dom": ">=18"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "homepage": "https://aisz.dev/",
  "prettier": {
    "singleQuote": false,
    "printWidth": 100,
    "trailingComma": "all"
  }
}
